---
import Layout from "../../../layouts/Layout.astro";

import backendProjects from "../../../config/backendProjects.json";
---

<Layout>
  <section
    class="modal frontend-modal animate__animated animate__zoomIn"
    id="backendModal"
  >
    <!--    CONTAINER   -->
    <div class="container">
      <div class="compu">
        <!--    OVNI    -->
        <a href="/en/#queHago" class="ovni-modal">
          <svg
            viewBox="0 0 893 934"
            id="volverAIndexBackend"
            class="ovni-creativo"
          >
            <g>
              <path
                d="M10.05,472.38C164.98,317.47,319.82,162.63,473.47,9c37.29,38.01,75.79,77.24,117.8,120.05
                            C478.39,240.56,362.08,355.47,242.43,473.68C359.86,590.59,474.99,705.22,589.28,819c-40.07,39.23-79.3,77.63-117.6,115.12
                            C319.48,781.89,164.63,627,10.05,472.38z"
              ></path>
              <path
                d="M889.31,653.34c-38.81,37.76-72.41,70.47-106.06,103.15c-5.21,5.05-10.59,9.92-14.47,13.55
