---
import FrontendGif from "../../media/img/gifs/frontend-min.gif";
import BackendGif from "../../media/img/gifs/backend-min.gif";
import { Image } from "astro:assets";

const { language } = Astro.props;
---

<section class="que-hago hidden" id="queHago">
  <!--    CONTENEDOR      -->
  <div class="contenedor-completo que-hago-contenedor">
    {language === "spanish" ? <h2>Hago...</h2> : <h2>I make...</h2>}

    <div class="contenido">
      <Image
        src={FrontendGif}
        alt=""
        class="animacion-servicios floating"
        id="animacionServiciosFrontend"
      />
      <Image
        src={BackendGif}
        alt=""
        class="animacion-servicios floating noMostrar"
        id="animacionServiciosBackend"
      />

      <div class="servicios">
        <!--    FRONTEND    -->
        <div class="servicios__section">
          <div class="servicios__section__title" id="frontendTitle">
            <h3 id="frontendText" class="selected">Frontend</h3>
            <i
              class="fa-solid fa-caret-down up-arrow selected"
              id="frontendArrow"></i>
          </div>

          <!--    TECNOLOGIAS FRONTEND    -->
          <div class="tecnologias__section" id="frontendContainer">
            <div class="tecnologias">
              <div class="tecnologias__tecnologia">
